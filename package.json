{
	"name": "frontend-github",
	"private": true,
	"version": "2.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"paraglide:generate": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"mock:oidc": "node samples/oidc-mock-server/server.mjs",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"postinstall": "[ \"$NODE_ENV\" = production ] && exit 0; paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"screenshots": "playwright test --config=playwright-screenshots.config.ts",
		"screenshots:ci": "PLAYWRIGHT_BASE_URL=http://localhost:3000 playwright test --config=playwright-screenshots.config.ts",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.1",
		"@inlang/paraglide-js": "^2.3.2",
		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.525.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/jwt": "^0.3.0",
		"@pdfslick/core": "^2.3.0",
		"@playwright/test": "^1.55.0",
		"@storybook/addon-a11y": "^9.1.6",
		"@storybook/addon-docs": "^9.1.6",
		"@storybook/addon-svelte-csf": "^5.0.8",
		"@storybook/addon-vitest": "^9.1.6",
		"@storybook/sveltekit": "^9.1.6",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "^2.41.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/vite": "^4.1.13",
		"@types/fhir": "^0.0.41",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.18.4",
		"@types/pg": "^8.15.5",
		"@unovis/svelte": "^1.6.1",
		"@unovis/ts": "^1.6.1",
		"bits-ui": "2.8.10",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dayjs": "^1.11.18",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.5",
		"eslint-plugin-storybook": "^9.1.6",
		"formsnap": "^2.0.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6",
		"lodash-es": "^4.17.21",
		"oauth2-mock-server": "^7.2.1",
		"openid-client": "^6.8.0",
		"pg": "^8.16.3",
		"pino": "^9.9.5",
		"pino-pretty": "^13.1.1",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"runed": "^0.29.2",
		"storybook": "^9.1.6",
		"sveaflet": "^0.1.4",
		"svelte": "^5.38.10",
		"svelte-breadcrumbs": "^2.0.1",
		"svelte-check": "^4.3.1",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.5",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.9.2",
		"uuid": "^11.1.0",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^6.3.6",
		"vite-plugin-kit-routes": "^1.0.2",
		"vitest": "^3.2.4",
		"zod": "^3.25.76"
	},
	"dependencies": {
		"mode-watcher": "^1.1.0"
	},
	"volta": {
		"node": "22.12.0",
		"pnpm": "9.15.4"
	},
	"packageManager": "pnpm@9.15.4",
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=9.0.0"
	},
	"pnpm": {
		"patchedDependencies": {
			"svelte-breadcrumbs": "patches/svelte-breadcrumbs.patch"
		},
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
	}
}
